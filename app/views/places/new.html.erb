<% provide(:title, "Place Register") %>
<h1>Register Place</h1>
<div class="row">
    <div class="span6 offset3">
        <%= form_for(@place) do |f| %>
            <%= f.label :name %>
            <%= f.text_field :name %>
            <%= f.label :address %>
            <%= f.text_field :address %>
           
            <%= f.submit "Register", class: "btn btn-large btn-primary" %>
        <% end %>
    </div>
</div>

